{
    "name": "carlossosa88/command-scheduler-bundle",
    "description": "This Symfony bundle will allow you to schedule all your commands just like UNIX crontab",
    "keywords": ["Symfony", "scheduler", "cron", "command", "task"],
    "homepage": "https://github.com/J-Mose/CommandSchedulerBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Julien Guyon",
            "email": "julienguyon@hotmail.com"
        },
        {
            "name": "Carlos Sosa",
            "email": "carlos@abc-tek.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "symfony/console": "^4.4",
        "doctrine/orm": "^2.7",
        "doctrine/doctrine-bundle": "^2.1",
        "carlossosa88/cron-expression": "v2.3.0-p2",
        "symfony/framework-bundle": "^4.4",
        "symfony/twig-bundle": "^4.4",
        "symfony/translation": "^4.4",
        "symfony/form": "^4.4",
        "symfony/asset": "^4.4",
        "symfony/templating": "^4.4",
        "symfony/validator": "^4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "php-coveralls/php-coveralls": "^2.0",
        "doctrine/doctrine-fixtures-bundle": "^3.0.0",
        "liip/functional-test-bundle": "^4.2",
        "symfony/css-selector": "^4.4",
        "symfony/security-bundle": "^4.4",
        "liip/test-fixtures-bundle": "^1.4.0"
    },
    "autoload": {
        "psr-4": {
            "JMose\\CommandSchedulerBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "Tests/"
        }
    }
}
